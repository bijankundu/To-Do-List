  <%- include("header"); -%>

  <body>
    <div class="box" id="heading">
      <h1><%= kindOfDay %></h1>
    </div>

    <div class="box">
      <% newListItems.forEach((ele,index)=>{ %>

      <div class="item">
        <input type="checkbox" />
        <p><%= ele %></p>
      </div>

      <%});%>

      <form action="/" method="POST" class="item">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off" />
        <button type="submit" name="button">+</button>
      </form>
    </div>
  </body>
  <%- include("footer"); -%>

  </html>